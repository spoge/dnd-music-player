{
  "name": "dnd-music-player",
  "description": "A music player made specifically for D&D",
  "author": "Sondre S Boge <sondreboge@hotmail.com> (http://sboge.no/)",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "src/start.js",
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "music-metadata-browser": "^1.8.1",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-player": "^1.14.2",
    "react-scripts": "3.2.0"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "electron-reload": "^1.5.0",
    "nodemon": "^2.0.1"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "react-start": "react-scripts start",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "package": "electron-packager . dnd-music-player --platform=win32 --arch=x64",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "asar": true,
    "appId": "no.sboge.dnd-music-player",
    "win": {
      "target": "nsis"
    }
  },
  "extends": null,
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
